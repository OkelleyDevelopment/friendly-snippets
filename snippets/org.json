{
  "blog": {
    "prefix": "\"blog\"",
    "body": [
      "\"#+STARTUP: showall indent\"",
      "\"#+STARTUP: hidestars\"",
      "\"#+BEGIN_HTML\"",
      "\"---\"",
      "\"layout: default\"",
      "\"title: ${1:title}\"",
      "\"excerpt: ${2:excerpt}\"",
      "\"---\"",
      "\"$0\""
    ],
    "description": "\"blog\""
  },
  "center block": {
    "prefix": "\"<C\"",
    "body": [
      "\"#+BEGIN_CENTER\"",
      "\"$0\"",
      "\"#+END_CENTER\""
    ],
    "description": "\"#+BEGIN_CENTER block\""
  },
  "code": {
    "prefix": "\"code\"",
    "body": [
      "\"#+BEGIN_${1:LANG} ${2:options}\"",
      "\"$0\"",
      "\"#+END_$1\""
    ],
    "description": "\"code\""
  },
  "comment block": {
    "prefix": "\"<c\"",
    "body": [
      "\"#+BEGIN_COMMENT\"",
      "\"$0\"",
      "\"#+END_COMMENT\""
    ],
    "description": "\"#+BEGIN_COMMENT block\""
  },
  "dot": {
    "prefix": "\"dot\"",
    "body": [
      "\"#+BEGIN_SRC dot :file ${1:file} :cmdline -T${2:pdf} :exports none :results silent\"",
      "\"$0\"",
      "\"#+END_SRC\"",
      "\"\"",
      "\"[[file:$1]]\""
    ],
    "description": "\"dot\""
  },
  "elisp": {
    "prefix": "\"elisp\"",
    "body": [
      "\"#+BEGIN_SRC emacs-lisp :tangle yes\"",
      "\"$0\"",
      "\"#+END_SRC\""
    ],
    "description": "\"elisp\""
  },
  "embedded": {
    "prefix": "\"emb\"",
    "body": [
      "\"src_${1:lang}${2:[${3:where}]}{${4:code}}\""
    ],
    "description": "\"embedded\""
  },
  "entry": {
    "prefix": "\"entry\"",
    "body": [
      "\"#+BEGIN_HTML\"",
      "\"---\"",
      "\"layout: ${1:default}\"",
      "\"title: ${2:title}\"",
      "\"---\"",
      "\"#+END_HTML\"",
      "\"$0\""
    ],
    "description": "\"entry\""
  },
  "example block": {
    "prefix": "\"<e\"",
    "body": [
      "\"#+BEGIN_EXAMPLE\"",
      "\"$0\"",
      "\"#+END_EXAMPLE\""
    ],
    "description": "\"#+BEGIN_EXAMPLE block\""
  },
  "export": {
    "prefix": "\"export\"",
    "body": [
      "\"#+BEGIN_EXPORT ${1:type}\"",
      "\"$0\"",
      "\"#+END_EXPORT\""
    ],
    "description": "\"export\""
  },
  "export ascii": {
    "prefix": "\"<a\"",
    "body": [
      "\"#+BEGIN_EXPORT ascii\"",
      "\"$0\"",
      "\"#+END_EXPORT\""
    ],
    "description": "\"#+BEGIN_EXPORT ascii\""
  },
  "export html": {
    "prefix": "\"<h\"",
    "body": [
      "\"#+BEGIN_EXPORT html\"",
      "\"$0\"",
      "\"#+END_EXPORT\""
    ],
    "description": "\"#+BEGIN_EXPORT html block\""
  },
  "export latex": {
    "prefix": "\"<l\"",
    "body": [
      "\"#+BEGIN_EXPORT latex\"",
      "\"$0\"",
      "\"#+END_EXPORT\""
    ],
    "description": "\"#+BEGIN_EXPORT latex block\""
  },
  "figure": {
    "prefix": "\"fig\"",
    "body": [
      "\"#+CAPTION: ${1:caption}\"",
      "\"#+ATTR_LATEX: ${2:scale=0.75}\"",
      "\"#+LABEL: fig:${3:label}\""
    ],
    "description": "\"figure\""
  },
  "header": {
    "prefix": "\"head\"",
    "body": [
      "\"#+TITLE:     ${1:Untitled Document}\"",
      "\"#+AUTHOR:    ${2:Author}\"",
      "\"#+DATE:      ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}\""
    ],
    "description": "\"org-file header\""
  },
  "html width": {
    "prefix": "null",
    "body": [
      "\"#+ATTR_HTML: :width ${1:500px}\""
    ],
    "description": "\"#+attr_html: :width ...\""
  },
  "image": {
    "prefix": "\"img\"",
    "body": [
      "\"#+ATTR_HTML: :alt $2 :align ${3:left} :class img\"",
      "\"[[${1:src}]${4:[${5:title}]}]\"",
      "\"$0\""
    ],
    "description": "\"img\""
  },
  "latex": {
    "prefix": "\"latex\"",
    "body": [
      "\"#+BEGIN_LATEX\"",
      "\"$0\"",
      "\"#+END_LATEX\""
    ],
    "description": "\"latex\""
  },
  "latex matrix": {
    "prefix": "\"matrix\"",
    "body": [
      "\"\\\\left \\\\(\"",
      "\"\\t\\\\begin{array}{${1:ccc}}\"",
      "\"\\t\\t${2:v1 & v2} \\\\\\\\\\\\\"",
      "\"\\t\\t$0\"",
      "\"\\t\\\\end{array}\"",
      "\"\\\\right \\\\)\""
    ],
    "description": "\"matrix\""
  },
  "quote block": {
    "prefix": "\"<q\"",
    "body": [
      "\"#+BEGIN_QUOTE\"",
      "\"$0\"",
      "\"#+END_QUOTE\""
    ],
    "description": "\"#+BEGIN_QUOTE block\""
  },
  "src block": {
    "prefix": "\"<s\"",
    "body": [
      "\"#+BEGIN_SRC ${1:lang}\"",
      "\"$0\"",
      "\"#+END_SRC\""
    ],
    "description": "\"#+BEGIN_SRC ... block\""
  },
  "todo": {
    "prefix": "\"todo\"",
    "body": [
      "\"TODO ${1:task description}\""
    ],
    "description": "\"TODO item\""
  },
  "verse": {
    "prefix": "\"<v\"",
    "body": [
      "\"#+BEGIN_VERSE\"",
      "\"$0\"",
      "\"#+END_VERSE\""
    ],
    "description": "\"verse\""
  }
}